{
  "name": "ngx-valdemort",
  "version": "13.0.0",
  "description": "Simple, consistent validation error messages for your Angular forms",
  "peerDependencies": {
    "@angular/common": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ninja-Squad/ngx-valdemort.git"
  },
  "keywords": [
    "angular",
    "form",
    "errors",
    "messages",
    "validation"
  ],
  "author": "Ninja Squad",
  "bugs": {
    "url": "https://github.com/Ninja-Squad/ngx-valdemort/issues"
  },
  "ng-add": {
    "save": "dependencies"
  },
  "dependencies": {
    "tslib": "2.8.1"
  },
  "module": "fesm2022/ngx-valdemort.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "default": "./fesm2022/ngx-valdemort.mjs"
    }
  },
  "sideEffects": false
}